{
  "name": "wtfx-news-now-generator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:port": "vite --port 5174",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node server/proxy-server.js",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "kill": "killall node || true && pkill -f vite || true",
    "start": "npm run kill && sleep 2 && npm run dev:full",
    "launch": "npm run kill && sleep 2 && concurrently \"npm run server\" \"npm run dev:port\" \"sleep 5 && open http://localhost:5174\"",
    "full": "killall node || true && sleep 3 && concurrently \"npm run server\" \"sleep 5 && npm run dev\" \"sleep 8 && open http://localhost:5173\""
  },
  "dependencies": {
    "@google/genai": "^1.15.0",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "googleapis": "^140.0.1",
    "html2canvas": "^1.4.1",
    "node-fetch": "^3.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "replicate": "^0.25.2",
    "sharp": "^0.33.4",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "wavesurfer.js": "^7.7.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  }
}
